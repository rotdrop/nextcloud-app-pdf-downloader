{
    "name": "gemorroj/archive7z",
    "description": "7z cli wrapper",
    "keywords": ["7z","archive","p7zip","7-zip","7zip"],
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/process": "^4.4||^5.0||^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Archive7z\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Archive7z\\Tests\\": "tests/"
        }
    }
}
